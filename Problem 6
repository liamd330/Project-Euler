# Done in Python

list = []
x = 100

for x in range(1, 101):
    s = x ** 2
    list.append(s)
    x - 1

print "List of squares from 1 through 100:", list
list = map(int, list)
print "Sum of the list:", sum(list)

list2 = []
for x in range(1, 101):
    list2.append(x)
    x - 1
print list2
list2 = map(int, list2)
print "Square of the sum:", sum(list2)**2

print sum(list2)**2 - sum(list)
